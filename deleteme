#the useAutomationExtention to bypass Admin
options = webdriver.ChromeOptions()
options.add_experimental_option('useAutomationExtension', False)
driver = webdriver.Chrome(ChromeDriverManager().install(), options=options)

appsec = AppSecH1Automation(driver)
appsec.load_file()

appsec.dashboardLogin(url="https://ssoadmin.ft.pdk.intcx.net/sso-admin/index.html", )

for email in appsec.data:
